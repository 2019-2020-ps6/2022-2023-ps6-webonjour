{
  "name": "front-end-back-office-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/front-end/back-office-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@mands/nx-playwright:playwright-executor",
      "options": {
        "e2eFolder": "apps/front-end/back-office-e2e",
        "devServerTarget": "front-end-back-office:serve",
        "packageRunner": "npx",
        "ui": true
      },
      "configurations": {
        "ci": {
          "ui": false,
          "devServerTarget": "front-end-back-office:serve:development"
        },
        "development": {
          "devServerTarget": "front-end-back-office:serve:development"
        },
        "docker": {
          "ui": true,
          "devServerTarget": null,
          "config": "playwright.docker.config.ts"
        }
      }
    },
    "ts-check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "tsc --build --force --verbose apps/front-end-back-office-e2e/tsconfig.json"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/front-end/back-office-e2e/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["front-end-back-office"]
}
